{
	"name": "xlsxtoarchivesink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_ADLS_GEN2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"filename1": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().filename1",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('shelter/archive/',substring(utcnow(),0,10))",
					"type": "Expression"
				},
				"fileSystem": "customerdatadomain"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}